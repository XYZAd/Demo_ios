source 'https://github.com/CocoaPods/Specs.git'

workspace 'XYZAdDemo.xcworkspace'

platform :ios, '9.0'

use_frameworks!
use_modular_headers!




targetsArray = ['XYZAdDemo']

targetsArray.each do |t|
    target t do
        pod 'Masonry'
        pod 'YYWebImage'
        
        pod 'XYZAd','1.5.2'
        #集成禾赞
        pod 'XYZAd/HZSDK', '1.5.2'
        #集成穿山甲
        pod 'XYZAd/JRTTSDK', '1.5.2'
        #集成百度
        pod 'XYZAd/BDSDK', '1.5.2'
        #集成广点通
        pod 'XYZAd/GDTSDK', '1.5.2'
        #集成MTG
        pod 'XYZAd/MTGSDK', '1.5.2'
        #集成SigmobAd
        pod 'XYZAd/SIGMOBSDK', '1.5.2'
        #集成快手
        pod 'XYZAd/KSMOBSDK', '1.5.2'
        #集成聚合
        pod 'XYZAd/JHSDK', '1.5.2'
        #集成游可盈
        pod 'XYZAd/YouKeYingSDK', '1.5.2'
        #集成京准通
        pod 'XYZAd/JDSDK', '1.5.2'
        #集成IronSource
        pod 'XYZAd/IronSourceSDK', '1.5.2'
        #集成AdColony
        pod 'XYZAd/AdColonySDK', '1.5.2'
        #集成友盟
        pod 'XYZAd/UMengSDK', '1.5.2'
        #集成UnityAds
        pod 'XYZAd/UnityAdSDK', '1.5.2'
        #集成Facebook
        pod 'XYZAd/FBAdSDK', '1.5.2'
        #集成谷歌SDK
        pod 'XYZAd/GADAdSDK', '1.5.2'
        #集成AppLovin
        pod 'XYZAd/ALSDK', '1.5.2'

    end
end


