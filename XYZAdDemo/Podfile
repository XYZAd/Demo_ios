source 'https://github.com/CocoaPods/Specs.git'

workspace 'XYZAdDemo.xcworkspace'

platform :ios, '9.0'

use_frameworks!
use_modular_headers!




targetsArray = ['XYZAdDemo']

targetsArray.each do |t|
    target t do
        pod 'Masonry'
        pod 'YYWebImage'
        pod 'BUAdTestMeasurement'
        pod 'XYZAd'

    end
end


